{
  "name": "mc-assets-editor",
  "version": "0.0.3",
  "description": "Minecraft Assets Editor",
  "main": "index.js",
  "scripts": {
    "build": "tsc && sass ./src/app.scss:./dist/app.css --style compressed",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./dist/main.js"
